<html>
   <script> 
let env = "dev"; 

let sendingAmount;

if (env === "dev") {
  sendingAmount = 100 + (100 * 0.10);
} else if (env === "test") {
  sendingAmount = 100 + (100 * 0.20);
} else if (env === "stage") {
  sendingAmount = 100 + (100 * 0.30);
} else if (env === "prod") {
  sendingAmount = 100 + (100 * 0.05);
} else {
  sendingAmount = 100;
}

console.log("Sending amount:", sendingAmount);
</script>
</html>


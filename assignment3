<html>
    <head>
    </head>
    <body>
        <script>
            let env ="dev";
            send_amount=100;

            if(env=="dev"){
                send_amount=send_amount+((send_amount/100)*10);
                console.log(send_amount);
            } 
            else if(env=="test"){
                send_amount=send_amount+((send_amount/100)*20);
                console.log(send_amount);}           
            else if(env=="stage"){
                send_amount=send_amount+((send_amount/100)*30);
                console.log(send_amount);}
            else if(env=="prod"){
                send_amount=send_amount+((send_amount/100)*40);
                console.log(send_amount);}

  

        </script>
    </body>
</html>

<html>
<script>
var env = "stage"; 
var sendingAmount;

switch (env) {
  case "dev":
    sendingAmount = 100 + (100 * 0.1);
    break;
  case "test":
    sendingAmount = 100 + (100 * 0.2);
    break;
  case "stage":
    sendingAmount = 100 + (100 * 0.3);
    break;
  case "prod":
    sendingAmount = 100 + (100 * 0.05);
    break;
  default:
    sendingAmount = 100; 
}

console.log("Sending amount:", sendingAmount);
</script>
</html>